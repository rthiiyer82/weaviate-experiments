{
  "compilerOptions": {
    "lib":["es5", "es6", "dom", "dom.iterable"],
    "target": "ES2019",
    "module": "commonjs",
    "baseUrl": "./E2ETests",
    "outDir": "./dist",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "resolveJsonModule": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": true,
    "esModuleInterop": true,
    "importHelpers": true,
    "preserveSymlinks": true,
    "paths": {
      "@utils": ["../E2ETests/util"],
      "@queries": ["E2ETests/query"],
      "@data": ["../movies.json"],
      "@shared-steps": ["./E2ETests/util/shared-steps"],
      "@interface": ["../E2ETests/interface"]
    }
  },
  "include": ["./E2ETests", ".eslintrc.js"]
}
